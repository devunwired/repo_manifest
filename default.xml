<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Element defining where to pull code from, can have more than one -->
  <remote  name="upstream"
           fetch="../devunwired" />
  <!-- Reference to the remote element to use when none specified for each project -->
  <default revision="refs/tags/v3"
           remote="upstream" />

  <!--
    Project elements: name must match remote repo name, path is destination directory for clone.
    May add a remote or revision attribute to override defaults.
    Revision can be branch, tag, or SHA
  -->
  <project path="platform/core" name="repo_platform" />
  <project path="platform/frameworks" name="repo_frameworks" revision="refs/tags/v2" />
  <project path="applications/main" name="repo_application" revision="experiment" />

</manifest>
